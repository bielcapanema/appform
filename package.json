{
  "name": "appform",
  "version": "1.0.0",
  "description": "Utilização de testes em vanilla JS",
  "main": "index.js",
  "scripts": {
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:8080 cy:run",
    "test:e2e:dev": "start-server-and-test start http://localhost:8080 cy:open",
    "pretest:e2e:run": "npm run build",
    "dev": "webpack-dev-server --mode=development",
    "build": "webpack --mode=production",
    "precommit": "lint-staged && npm run test:e2e:run",
    "postbuild": "cp ./public/index.html ./dist/index.html",
    "start": "npm-run-all --parallel start:*",
    "start:client": "serve --no-clipboard --single --listen 8080 dist",
    "format": "prettier \"**/*.+(js|json|less|css|ts|tsx|md|scss)\" --write",
    "lint": "jest --config test/jest.lint.js",
    "validate": "npm run test:coverage && npm run test:e2e:run",
    "netlify": "serve test:coverage build && cp -r coverage/lcov-report dist/lcov-report",
    "setup": "npm install && npm run validate"
  },
  "keywords": [],
  "author": "Gabriel Capanema <capanema.ggfc@gmail.com>",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/core": "^7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-loader": "^8.0.2",
    "babel-plugin-dynamic-import-node": "^2.1.0",
    "babel-polyfill": "^6.26.0",
    "css-loader": "^1.0.0",
    "cypress": "^3.1.0",
    "cypress-testing-library": "^2.2.1",
    "eslint": "^5.6.0",
    "eslint-config-cypress": "^0.28.0",
    "eslint-config-kentcdodds": "^14.0.4",
    "eslint-import-resolver-jest": "^2.1.1",
    "file-loader": "^2.0.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "is-ci-cli": "^1.1.1",
    "jest": "^23.6.0",
    "jest-runner-eslint": "^0.6.0",
    "jest-watch-select-projects": "^0.1.1",
    "jest-watch-typeahead": "^0.2.0",
    "lint-staged": "^7.3.0",
    "node-sass": "^4.9.4",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "prop-types": "^15.6.2",
    "sass-loader": "^7.1.0",
    "serve": "^10.0.2",
    "start-server-and-test": "^1.7.7",
    "style-loader": "^0.23.0",
    "test-data-bot": "^0.6.0",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bielcapanema/appform.git"
  },
  "bugs": {
    "url": "https://github.com/bielcapanema/appform/issues"
  },
  "homepage": "https://github.com/bielcapanema/appform#readme"
}
